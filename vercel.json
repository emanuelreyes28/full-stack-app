{
  "version": 2,
  "buildCommand": "cd fav-links-react && npm install && npm run build",
  "builds": [
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/links(.*)",
      "dest": "server/index.js"
    },
    {
      "src": "/test",
      "dest": "server/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "server/index.js"
    }
  ]
}

